{
    "name" : "nemo64/typo3api",
    "authors": [
        {
            "name": "Marco Pfeiffer",
            "role": "Developer",
            "homepage": "https://www.marco.zone/"
        }
    ],
    "config": {
        "platform": {
            "php": "7.0.15"
        }
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": "web"
        }
    },
    "require": {
        "php": "^7.0",
        "typo3/cms": "8.7.*",
        "symfony/options-resolver": "^3.3",
        "symfony/polyfill-mbstring": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Typo3Api\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Typo3Api\\": "tests"
        }
    }
}
